# Jenkins Environment Variables for Production
NODE_ENV=production
MONGO_ROOT_USERNAME=prod_user
MONGO_ROOT_PASSWORD=super_secure_production_password_456
PRODUCTION_DOMAIN=myapp.com

# Application Configuration
APP_NAME=jenkins-mern-app-production
LOG_LEVEL=warn

# Deployment
DEPLOY_ENV=production
BUILD_NUMBER=${BUILD_NUMBER}
GIT_COMMIT=${GIT_COMMIT}

# Security
JWT_SECRET=production_jwt_secret_very_secure
CORS_ORIGIN=https://myapp.com

# Monitoring
SLACK_WEBHOOK_URL=your_production_slack_webhook